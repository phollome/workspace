{
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": { "defaultBase": "master" },
  "npmScope": "phollome",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "react-hooks": { "tags": ["scope:public"] },
    "react-components": { "tags": [] },
    "tailwind-styles": { "tags": [] },
    "wfa-list-of-literature": { "tags": [] },
    "stores-api": { "tags": [] },
    "stores": { "tags": [] },
    "list-of-literature-api": { "tags": [] }
  }
}
